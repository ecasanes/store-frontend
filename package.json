{
  "name": "mercury",
  "version": "0.0.0",
  "main": "dist/main.js",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "web": "ng build --base-href .",
    "start": "npm run build-electron && electron dist/main.js",
    "desktop": "npm run build-electron && electron dist/main.js",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build-electron": "ng build --base-href . && cp src/electron/* dist",
    "build-electron-prod": "ng build --base-href . --prod --environment=prod && cp src/electron/* dist",
    "package-mac": "npm run build-electron && electron-packager . --overwrite --asar=true --platform=darwin --arch=x64 --prune=true --out=release-builds --icon=src/assets/icons/mac/icon.icns",
    "package-windows": "npm run build-electron && electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --icon=src/assets/icons/win/icon.ico --version-string.CompanyName=Webforest --version-string.ProductName=\"Mercury App\"",
    "package-mac-prod": "npm run build-electron-prod && electron-packager . --overwrite --asar=true --platform=darwin --arch=x64 --prune=true --out=release-builds --icon=src/assets/icons/mac/icon.icns",
    "package-windows-prod": "npm run build-electron-prod && electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --icon=src/assets/icons/win/icon.ico --version-string.CompanyName=Webforest --version-string.ProductName=\"Mercury App\"",
    "package-linux": "npm run build-electron && electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/mercury-darwin-x64/mercury.app electron-boilerplate-app --out=release-builds --overwrite",
    "create-installer-windows": "node installers/windows/createinstaller.js",
    "full-build-mac": "npm run build-electron && npm run package-mac && npm run create-installer-mac",
    "full-build-windows": "npm run build-electron-prod && npm run package-windows && npm run create-installer-windows",
    "mac-test": "npm run package-mac-prod && npm run create-installer-mac",
    "installforge-build": "npm run package-windows-prod",
    "ernest-test": "npm run package-windows-prod && npm run create-installer-windows",
    "ernest-package-test": " electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --icon=src/assets/icons/win/icon.ico --version-string.CompanyName=Webforest --version-string.ProductName=\"Mercury App\"",
    "electron": "electron dist/main.js",
    "electron-test": "cp src/electron/* dist && electron dist/main.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.1",
    "@angular/common": "^4.4.1",
    "@angular/compiler": "^4.4.1",
    "@angular/compiler-cli": "^4.4.1",
    "@angular/core": "^4.4.1",
    "@angular/forms": "^4.4.1",
    "@angular/http": "^4.4.1",
    "@angular/platform-browser": "^4.4.1",
    "@angular/platform-browser-dynamic": "^4.4.1",
    "@angular/platform-server": "^4.4.1",
    "@angular/router": "^4.4.1",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.5",
    "core-js": "^2.4.1",
    "dotenv": "^4.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.2.1",
    "ng2-slugify": "^0.1.0",
    "ng2-truncate": "^1.3.11",
    "ngx-sweetalert2": "^0.2.4",
    "ngx-toastr": "^6.3.0",
    "pusher": "^1.5.1",
    "pusher-js": "^4.2.1",
    "rxjs": "^5.2.0",
    "sweetalert2": "^6.9.1",
    "systemjs": "^0.19.47",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.2",
    "@angular/compiler-cli": "^2.3.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.60",
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "^3.1.0",
    "electron": "^1.7.6",
    "electron-installer-dmg": "^0.2.1",
    "electron-packager": "^9.0.1",
    "electron-winstaller": "^2.6.3",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.5",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "lite-server": "^2.3.0",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "^2.3.4"
  }
}
